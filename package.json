{
  "name": "@khs1994/tencent-ai",
  "version": "19.6.0-alpha.2",
  "description": "Tencent AI SDK",
  "main": "./dist/tencent-ai.common.js",
  "types": "./types/TencentAI.d.ts",
  "module": "./dist/tencent-ai.esm.js",
  "unpkg": "dist/tencent-ai.min.js",
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/util/wxFs.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "scripts": {
    "cleanup": "npx rimraf lib libesm dist types && git checkout -- dist",
    "eslint": "eslint ./src ./test",
    "test": "nyc mocha -R spec -r ts-node/register --timeout 20000 test/*.ts test/**/*.ts",
    "precodecov": "nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "npx codecov",
    "fmt": "npx prettier --write ./**/*.js ./**/*.ts ./*.md ./*.json",
    "prepublishOnly": "npm run cleanup && npm run dist:all",
    "dev:miniprogram": "npx tsc --outDir ../../app/pcit/miniprogram/client/node_modules/@khs1994/tencent-ai/lib",
    "build:all": "npx rimraf lib libesm && npm run build:esm && npm run build:cjs",
    "build:esm": "npx tsc --module es2015 --outDir libesm",
    "build:cjs": "npx tsc",
    "dist:all": "npx rimraf dist && git checkout -- dist && npm run dist:umd && npm run dist:umd:min && npm run dist:cjs && npm run dist:cjs:min && npm run dist:esm",
    "dist:umd": "npx rollup -c rollup.config.js --environment=target:umd,NODE_ENV:development",
    "dist:umd:min": "npx rollup -c rollup.config.js --environment=NODE_ENV:production",
    "dist:cjs": "npx rollup -c rollup.config.js --environment=target:cjs,NODE_ENV:development",
    "dist:cjs:min": "npx rollup -c rollup.config.js --environment=target:cjs_min,NODE_ENV:production",
    "dist:esm": "npx rollup -c rollup.config.js --environment=target:esm,NODE_ENV:development"
  },
  "author": {
    "name": "khs1994",
    "email": "khs1994@khs1994.com",
    "url": "https://khs1994.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khs1994/tencent-ai-js.git"
  },
  "license": "MIT",
  "keywords": [
    "tencent-ai",
    "ai",
    "sdk"
  ],
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.0",
    "@types/wx": "github:khs1994/types-wx",
    "babel-eslint": "^10.0.3",
    "codecov": "^3.5.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "iconv-lite": "^0.5.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "qs": "^6.8.0",
    "rollup": "^1.21.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3",
    "typescript-eslint-parser": "^22.0.0",
    "wx-fetch": "github:khs1994/wx-fetch"
  },
  "files": [
    "dist",
    "types",
    ".pcit.yml"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "homepage": "https://github.com/khs1994/tencent-ai-js",
  "bugs": {
    "url": "https://github.com/khs1994/tencent-ai-js/issues"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./src"
  }
}
