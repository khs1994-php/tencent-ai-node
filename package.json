{
  "name": "@khs1994/tencent-ai",
  "version": "18.12.0-alpha.1",
  "description": "Tencent AI SDK",
  "main": "./lib/TencentAI.js",
  "types": "./types/TencentAI.d.ts",
  "scripts": {
    "eslint": "eslint ./src ./test",
    "test": "nyc mocha -R spec -r ts-node/register --timeout 10000 ./test/*.ts ./test/**/*.ts",
    "precodecov": "nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "./node_modules/.bin/codecov",
    "prettier": "npx prettier --write ./**/*.js ./**/*.ts ./*.md",
    "webpack-pro": "npx webpack-cli --mode=production",
    "prepublishOnly": "npx rimraf lib dist types && npm run webpack-pro && npx tsc",
    "dev-miniprogram": "npx tsc --outDir ../../app/pcit/miniprogram/client/node_modules/@khs1994/tencent-ai/lib"
  },
  "author": {
    "name": "khs1994",
    "email": "khs1994@khs1994.com",
    "url": "https://khs1994.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khs1994-php/tencent-ai-node.git"
  },
  "license": "MIT",
  "keywords": [
    "tencent-ai",
    "ai",
    "sdk"
  ],
  "dependencies": {
    "node-fetch": "^2.3.0",
    "qs": "^6.6.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.11",
    "@types/wx": "github:khs1994/types-wx",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "iconv-lite": "^0.4.19",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2",
    "ts-loader": "^5.3.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1",
    "typescript-eslint-parser": "^21.0.1"
  },
  "files": [
    "lib",
    "dist",
    "src",
    "types",
    ".pcit.yml"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "private": false,
  "homepage": "https://github.com/khs1994-php/tencent-ai-node",
  "bugs": {
    "url": "https://github.com/khs1994-php/tencent-ai-node/issues"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./lib",
    "src": "./src"
  }
}
