{
  "name": "@khs1994/tencent-ai",
  "version": "19.6.0-alpha.1",
  "description": "Tencent AI SDK",
  "main": "./lib/TencentAI.js",
  "types": "./types/TencentAI.d.ts",
  "module": "./libesm/TencentAI.js",
  "unpkg": "dist/tencent-ai.min.js",
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/util/wxFs.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "scripts": {
    "cleanup": "npx rimraf lib libesm dist types",
    "eslint": "eslint ./src ./test",
    "test": "nyc mocha -R spec -r ts-node/register --timeout 20000 test/*.ts test/**/*.ts",
    "precodecov": "nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "npx codecov",
    "fmt": "npx prettier --write ./**/*.js ./**/*.ts ./*.md ./*.json",
    "prepublishOnly": "npm run cleanup && npm run dist:all && npm run build:all",
    "dev:miniprogram": "npx tsc --outDir ../../app/pcit/miniprogram/client/node_modules/@khs1994/tencent-ai/lib",
    "build:all": "npx rimraf lib libesm && npm run build:esm && npm run build:cjs",
    "build:esm": "npx tsc --module es2015 --outDir libesm",
    "build:cjs": "npx tsc",
    "dist:all": "npx rimraf dist && npm run dist:umd && npm run dist:umd:min && npm run dist:cjs && npm run dist:esm",
    "dist:umd": "npx rollup -c rollup.config.js --environment=target:umd",
    "dist:umd:min": "npx rollup -c rollup.config.js",
    "dist:cjs": "npx rollup -c rollup.config.js --environment=target:cjs",
    "dist:esm": "npx rollup -c rollup.config.js --environment=target:esm"
  },
  "author": {
    "name": "khs1994",
    "email": "khs1994@khs1994.com",
    "url": "https://khs1994.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khs1994-php/tencent-ai-node.git"
  },
  "license": "MIT",
  "keywords": [
    "tencent-ai",
    "ai",
    "sdk"
  ],
  "dependencies": {
    "node-fetch": "^2.4.1",
    "qs": "^6.7.0",
    "wx-fetch": "github:khs1994/wx-fetch"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.0.0",
    "@types/wx": "github:khs1994/types-wx",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "iconv-lite": "^0.4.19",
    "mocha": "^6.0.0",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.0.0",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0"
  },
  "files": [
    "lib",
    "libesm",
    "dist",
    "types",
    ".pcit.yml"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "private": false,
  "homepage": "https://github.com/khs1994-php/tencent-ai-node",
  "bugs": {
    "url": "https://github.com/khs1994-php/tencent-ai-node/issues"
  },
  "directories": {
    "doc": "./docs",
    "lib": [
      "./lib",
      "./libesm"
    ],
    "src": "./src"
  }
}
